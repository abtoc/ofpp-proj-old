extends layout.pug

block title
    |ペタペタシステム
block content
    table.table.table-hover
        thead
            tr
                th 名前
                td
        tbody
            for item in items
                tr
                    td= item['name']
                    td(align="right")
                        .btn-group
                            if not item['staff']
                                a.btn.btn-info(href="{{ url_for('index') }}") 実績記録
